{
  "name": "b2proof",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:test": "dotenv -e .env.test -- next dev",
    "build": "next build && next-sitemap",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "postbuild": "next-sitemap",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
